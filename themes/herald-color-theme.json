{
  "name": "Herald",
  "type": "dark",
  "colors": {
    "editor.background": "#1f1f1f",
    "editor.foreground": "#d0d0d0",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#696567"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "support.constant",
        "constant"
      ],
      "settings": {
        "foreground": "#6DF584"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#FFB539"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type", "entity.name.type", "meta.tag"],
      "settings": {
        "foreground": "#FFEE68"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FC4234"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#E783E9"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#FC6984"
      }
    },
    {
      "name": "Doctype",
      "scope": ["meta.tag.sgml", "meta.tag.sgml.html"],
      "settings": {
        "foreground": "#696567"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#90CBF1"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#70BDF1"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#FF5370"
      }
    }
  ]
}
